##### scalar
1+1
3-2
3*2
3/2

a <- 1 + 1
a
b<-3
a+b
a^b
10+a

log(10)
log(10, base=10)

##### vector
a<-c(1,2,3)
a<-1:3
a

a[2]
a[2:3]
a[c(2:3)]

a2<-c("A","B","C")
a2

##### matrix
b<-matrix(c(1,2,3,4,5,6),nrow=2,ncol=3)
b
t(b)
c<-matrix(c(1,2,3,4),nrow=2,ncol=2)
solve(c)
det(c)
c%*%b
b[2,3]
b[2,]
b[1,1:2]

##### data.frame
d<-data.frame(x=c(10,20,30),y=c("A","B","C"))
d
names(d)
names(d)<-c("num","text")
d

##### list
e<-list(a2,b,d)
e
e[[2]]
e[[2]][,1]
